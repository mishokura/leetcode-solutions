class Solution(object):
    def twoSum(self, nums, target):
        """
        :type nums: List[int]
        :type target: int
        :rtype: List[int]
        """
        numlen = len(nums)

        for i in range(numlen):
            for j in range(numlen):
                if i!=j:
                    if nums[i] + nums[j] == target:
                        return [i, j]

        return False
